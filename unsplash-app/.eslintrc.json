{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        ["react-app","prettier"],
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "plugins": [
        "react","prettier"
    ],
    "rules": {
        "prettier/prettier" : [
            "error",{
                "useTabs":true,
                "semi":true
            }
        ]
    }
}